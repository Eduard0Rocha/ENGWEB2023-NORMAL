extends layout

block content
    .w3-card-4
    header.w3-container.w3-teal
      h1 Espécie: #{list[0].Espécie} 
      h2 Nome Científico: #{list[0]['Nome Científico']}
      a(href="/") Voltar
    .w3-card-4
      w3-container
        table.w3-table-all 
          tr
            th _id
            th Freguesia
            th Estado 
            th Número de intervenções 
          
          each item in list
              tr 
                td #{item.Id}
                td #{item.Freguesia}
                td #{item.Estado}
                td #{item['Número de intervenções']}